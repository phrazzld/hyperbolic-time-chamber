# Appfile for WorkoutTracker
# App-specific configuration for Fastlane

# Apple Developer Portal Team ID
team_id(ENV["TEAM_ID"])

# App Store Connect Team ID (if different from Developer Portal)
itc_team_id(ENV["ITC_TEAM_ID"] || ENV["TEAM_ID"])

# Bundle identifier for the app
app_identifier("com.yourcompany.WorkoutTracker")

# Apple ID for the developer account
apple_id(ENV["APPLE_ID"])

# For more information about the Appfile, see:
# https://docs.fastlane.tools/advanced/#appfile